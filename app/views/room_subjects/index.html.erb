<p id="notice"><%= notice %></p>

<h1>Listing Room Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_subjects.each do |room_subject| %>
      <tr>
        <td><%= room_subject.name %></td>
        <td><%= link_to 'Show', room_subject %></td>
        <td><%= link_to 'Edit', edit_room_subject_path(room_subject) %></td>
        <td><%= link_to 'Destroy', room_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room subject', new_room_subject_path %>
